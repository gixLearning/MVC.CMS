@model MVC.CMS.Models.ListUsersViewModel

<p>
    <h4>User management</h4>
</p>
<table class="table">
    @*<tr>
        <th></th>
        <th></th>
    </tr>*@

@foreach (var item in Model.UserList) {
    <tr>
        <td>
            @item.UserName
        </td>
        <td>
            @Html.ActionLink("Edit", "EditUser", new { id=item.UserId }) |
            @Html.ActionLink("Delete", "DeleteUser", new { id=item.UserId })
        </td>
    </tr>
}

</table>
